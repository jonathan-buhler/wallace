{
    "name": "wallace",
    "version": "1.0.0",
    "description": "A script to help find you a Tesco delivery slot",
    "main": "wallace.js",
    "author": "Jonathan Buhler <benedict.buhler@gmail.com>",
    "license": "GPL-3.0",
    "private": true,
    "proxy": "http://localhost:8080",
    "dependencies": {
        "chalk": "^4.0.0",
        "lodash": "^4.17.15",
        "moment": "^2.24.0",
        "node-cron": "^2.0.3",
        "ora": "^4.0.4",
        "prompts": "^2.3.2",
        "puppeteer": "^2.1.1"
    },
    "devDependencies": {
        "husky": "^4.2.5",
        "prettier": "^2.0.4",
        "pretty-quick": "^2.0.1"
    },
    "prettier": {
        "tabWidth": 4,
        "printWidth": 120
    },
    "scripts": {
        "spin": "node -e 'require(\"./src/server\").serve()'",
        "reset": "node -e 'require(\"./src/user\").clearUser()'",
        "update": "git pull"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn pretty-quick --staged"
        }
    }
